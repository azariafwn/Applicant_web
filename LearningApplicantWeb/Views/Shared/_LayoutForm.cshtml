<!doctype html>
<html lang="en" data-layout="vertical" data-topbar="light" data-sidebar="dark" data-sidebar-size="lg">

<head>

    <meta charset="utf-8" />
    <title>@ViewData["Title"] | Bapenda Surabaya</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta content="Premium Multipurpose Admin & Dashboard Template" name="description" />
    <meta content="Themesbrand" name="author" />
    <!-- App favicon -->
    <link rel="shortcut icon" href="~/assets/images/favicon.ico">

    <script src="~/assets/js/layout.js"></script>
    <link href="~/assets/js/jquery-ui.css" rel="stylesheet" />
    <link href="~/assets/css/bootstrap.min.css" rel="stylesheet" type="text/css" />
    <link href="~/assets/css/icons.min.css" rel="stylesheet" type="text/css" />
    <link href="~/assets/css/app.min.css" rel="stylesheet" type="text/css" />
    <link href="~/assets/css/custom.min.css" rel="stylesheet" type="text/css" />
    <link href="~/assets/datatables/datatables.min.css" rel="stylesheet" />
    <link href="~/assets/libs/sweetalert2/sweetalert2.min.css" rel="stylesheet" />

    <link href="~/assets/css/spinner.css" id="app-style" rel="stylesheet" type="text/css" />

    <style type="text/css">
        .card-preloader {
           position: fixed;
           top: 0;
           left: 0;
           width: 100%;
           height: 100%;
           z-index: 9999;
           background: #ffffff;
        }

        #spiner {
           position: absolute;
           left: 50%;
           top: 40%;
           transform: translate(-50%,-50%);
        }

        @@keyframes spin {
           to {
              transform: rotate(360deg)
           }
        }

        .required-star:after {
           content: " *";
           color: red;
        }

        #loading1 {
           background-color: black;
           opacity: 0.5;
           z-index: 9999;
           top: 0;
           bottom: 0;
           left: 0;
           right: 0;
           position: fixed;
        }

        .spinner, .spinner::before, .spinner::after {
           content: '';
           display: block;
           border: 4px solid transparent;
           border-radius: 50%;
           animation: spin infinite linear 2s;
           position: absolute;
        }

        .spinner {
           width: 100px;
           height: 100px;
           border-top-color: orange;
           top: calc(50% - 50px);
           left: calc(50% - 50px);
        }

        .teks h2 {
           font-family: Calibri;
           color: #a9a4ff;
           position: fixed;
           margin-top: 15px;
           margin-left: -20px;
           top: calc(50% - 50px);
           left: calc(50% - 50px);
           text-align: center;
        }

        .spinner::before {
           border-top-color: magenta;
           animation-duration: 4s;
           top: 2px;
           bottom: 2px;
           left: 2px;
           right: 2px;
        }

        .spinner::after {
           border-top-color: cyan;
           top: 8px;
           bottom: 8px;
           left: 8px;
           right: 8px;
        }

        .blink {
           animation: blinker 3s linear infinite;
        }

        @@keyframes blinker {
           0% {
              opacity: 0;
           }

           50% {
              opacity: .5;
           }

           100% {
              opacity: 1;
           }
        }
    </style>
</head>

<body data-bs-spy="scroll" data-bs-target="#navbar-example">
    <div class="layout-wrapper landing">

        @await Html.PartialAsync("_Modal")
        <!-- start hero section -->
        <section class="section pb-0 hero-section" id="hero">
            <div class="bg-overlay bg-overlay-pattern"></div>
            <div class="container">
                <div class="row justify-content-center">
                    <div class="col-lg-8 col-sm-10">
                        <div class="text-center mt-lg-5 pt-5">
                            <h1 class="display-6 fw-semibold mb-3 lh-base text-success">
                                FORM PENDAFTARAN
                            </h1>
                        </div>
                    </div>
                </div>
                <!-- end row -->
            </div>
        </section>

        <section class="section pb-0" id="tarikan section">
            <div class="bg-overlay bg-overlay-pattern"></div>
            <div class="container">
                @RenderBody()
            </div>
        </section>


    </div>
    <!-- end layout wrapper -->

    <script src="~/assets/libs/simplebar/simplebar.min.js"></script>
    <script src="~/assets/libs/node-waves/node-waves.min.js"></script>
    <script src="~/assets/libs/feather-icons/feather-icons.min.js"></script>
    <script src="~/assets/js/pages/plugins/lord-icon-2.1.0.min.js"></script>
    <script src="~/assets/js/plugins.min.js"></script>

    <script src="~/assets/js/jquery.js"></script>
    <script src="~/assets/js/jquery.validate.js"></script>
    <script src="~/assets/js/jquery-ui.js"></script>
    <script src="~/assets/datatables/datatables.min.js"></script>

    <script src="~/assets/libs/sweetalert2/sweetalert2.min.js"></script>
    <script src="~/assets/js/spinner.js"></script>

    <!-- App js -->
    @await RenderSectionAsync("Scripts", required: false)
</body>

</html>